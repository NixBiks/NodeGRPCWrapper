{
  "name": "wrapper",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "exports": {
    "./": "./src/generated/"
  },
  "typesVersions": {
    "*": {
      "hello_grpc_pb": [
        "./src/generated/hello_grpc_pb.d.ts"
      ]
    }
  },
  "scripts": {
    "generate": "grpc_tools_node_protoc --proto_path=. --js_out=\"import_style=commonjs,binary:src/generated\" --grpc_out=\"grpc_js:src/generated\" --ts_out=\"service=grpc-node,mode=grpc-js:src/generated\" \"hello.proto\""
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7"
  },
  "devDependencies": {
    "grpc-tools": "^1.11.2",
    "ts-protoc-gen": "^0.15.0"
  }
}
